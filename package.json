{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "format": "prettier --config .prettierrc.cjs -uw ./src/ ./CICD/ ./public/config/yaml/ *.md",
    "build:json": "node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json; vite build",
    "preview": "pnpm build; vite preview --host",
    "check": "eslint --config ./.eslintrc.json ./src/ ./CICD/ --ext .js,.vue"
  },
  "dependencies": {
    "axios": "1.1.3",
    "fastest-levenshtein": "1.0.16",
    "pinia": "2.0.23",
    "pinia-plugin-persistedstate": "2.3.0",
    "vue": "3.2.41",
    "vue-router": "4.1.5"
  },
  "devDependencies": {
    "@trivago/prettier-plugin-sort-imports": "3.3.1",
    "@vitejs/plugin-legacy": "2.2.0",
    "@vitejs/plugin-vue": "3.1.2",
    "chalk": "5.1.2",
    "eslint": "8.26.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-sort-exports": "0.7.0",
    "eslint-plugin-vue": "9.6.0",
    "js-yaml": "4.1.0",
    "postcss": "8.4.18",
    "prettier": "2.7.1",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.8.3",
    "sass": "1.55.0",
    "terser": "5.15.1",
    "vite": "3.1.8",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.13.1"
  }
}
