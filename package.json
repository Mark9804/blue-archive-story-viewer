{
  "name": "blue-archive-story-viewer",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --host",
    "format": "prettier --config .prettierrc.json -uw ./src/ ./CICD/ ./public/config/yaml/ *.md",
    "build:json": "eslint --fix --config ./.eslintrc.json ./public/config/yaml/ && node ./CICD/build_json.js",
    "build": "pnpm format; pnpm build:json && vue-tsc --noEmit --skipLibCheck && vite build",
    "build:github": "pnpm build; cp ./dist/index.html ./dist/404.html",
    "preview": "pnpm build:json && vite build; vite preview --host",
    "check": "eslint --fix --config ./.eslintrc.json ./src/ ./CICD/ ./public/config/yaml/ --ext .js,.ts,.vue,.yml,.yaml && vue-tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@pixi-spine/base": "^3.1.0",
    "@pixi/constants": ">=6.1.0 <7.0.0",
    "@pixi/core": "6.5.9",
    "@pixi/display": ">=6.1.0 <7.0.0",
    "@pixi/extensions": "6.5.9",
    "@pixi/filter-adjustment": "^4.0.0",
    "@pixi/filter-color-overlay": "^4.0.0",
    "@pixi/filter-crt": "^4.0.0",
    "@pixi/filter-motion-blur": "^4.2.0",
    "@pixi/graphics": "^7.0.0",
    "@pixi/loaders": ">=6.1.0 <7.0.0",
    "@pixi/math": "6.5.9",
    "@pixi/mesh": "6.5.9",
    "@pixi/mesh-extras": "^6.1.0",
    "@pixi/particle-emitter": "^5.0.8",
    "@pixi/runner": "6.5.9",
    "@pixi/settings": "6.5.9",
    "@pixi/sound": "<5.0.0",
    "@pixi/sprite": ">=6.1.0 <7.0.0",
    "@pixi/ticker": ">=6.0.4 <7.0.0",
    "@pixi/utils": ">=6.1.0 <7.0.0",
    "@vue/compiler-sfc": "3.x",
    "@vueuse/core": "^9.13.0",
    "axios": "1.3.4",
    "axios-retry": "3.4.0",
    "ba-story-player": "^0.3.0",
    "fastest-levenshtein": "1.0.16",
    "gsap": "^3.11.3",
    "mitt": "^3.0.0",
    "pinia": "2.0.32",
    "pinia-plugin-persistedstate": "3.1.0",
    "pixi-spine": "^3.1.0",
    "pixi.js": "^6.0.0",
    "toastify-js": "1.12.0",
    "typed.js": "^2.0.12",
    "vue": "3.2.47",
    "vue-router": "4.1.6",
    "xxhashjs": "^0.2.2"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "1.2.0",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "^18.11.9",
    "@types/toastify-js": "1.11.1",
    "@types/xxhashjs": "^0.2.2",
    "@typescript-eslint/eslint-plugin": "5.54.0",
    "@typescript-eslint/parser": "5.54.0",
    "@vitejs/plugin-legacy": "4.0.1",
    "@vitejs/plugin-vue": "4.0.0",
    "@vue/eslint-config-typescript": "11.0.2",
    "chalk": "5.2.0",
    "eslint": "8.35.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-sort-exports": "0.8.0",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-yaml": "0.5.0",
    "js-yaml": "4.1.0",
    "postcss": "8.4.21",
    "postcss-plugin-px2rem": "0.8.1",
    "postcss-preset-env": "8.0.1",
    "prettier": "2.8.4",
    "prettier-plugin-css-order": "1.3.0",
    "rollup-plugin-visualizer": "5.9.0",
    "sass": "1.58.3",
    "terser": "5.16.5",
    "typescript": "4.9.5",
    "vite": "4.1.4",
    "vite-plugin-compression": "0.5.1",
    "vite-plugin-pwa": "0.14.4",
    "vue-tsc": "1.2.0",
    "workbox-window": "6.5.4"
  }
}
